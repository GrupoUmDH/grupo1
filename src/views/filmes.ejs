<body>
    <%- include('partials/header') %>
        <main>

            <section class="container">
                <div class="titulos">
                    <h1>FILMES</h1>
                </div>

                <hr>
                <section class="filmes-home">
                    <% for(const filmes of filmes) { %>
                        <div>
                            <img src="img/filmes/<%=filmes.imagem%>.jpg">
                            <p>
                                <%= filmes.nome %>
                            </p>
                            <div class="preco"><span>
                                    <%= categorias.valor_aluguel %>
                                </span></div>
                            <div class="preco"><span>
                                    <%= categorias.valor_compra %>
                                </span></div>
                        </div>
                        <% }%>
                </section>
            </section>
        </main>
        <%- include('partials/footer')%>

</body>

</html>