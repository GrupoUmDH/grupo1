<%- include('partials/header') %>
    <main>

        <section class="container">
            <div class="titulos">
                <h1>FILMES</h1>
            </div>

            <hr>
            <section class="filmes-home">
                <% for(const filmes of Filmes) { %>
                    <div class="filme-show">
                        <img src="/img/filmes/<%=filmes.imagem%>.jpg">
                        <div class="descri">
                            <p id="nome">
                                <%= filmes.nome %>
                            </p>
                            <p id="descri-filme">
                                <%= filmes.descricao %>
                            </p>
                        </div>
                    </div>
                    <% }%>
            </section>
        </section>
    </main>
    <%- include('partials/footer')%>