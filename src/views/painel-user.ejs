            <section class="altera-dados">
                <div>
                    <h1 id="alterar" class="h1-t">Alterar Dados</h1>
                </div>
    
                <section class="alterar">
                    <div class="formulario">
    
                        <div class="dados-usuario">
    
    
                            <form method="post" id="form-email">
    
                                <div class="invisible err-email">Campo obrigatório!</div>
                                <input placeholder="DIGITE NOVO E-MAIL" type="email" name="email" id="email">
                                <div class="invisible err-email1">Confirme o novo e-mail Digitado.</div>
                                <input placeholder="DIGITE NOVO E-MAIL NOVAMENTE" type="email" name="email1" id="email1">
                                <button id="alterar-email" class="btn-form">Alterar</button>
                            </form>
                            <form method="post" id="form-nome">
    
                                <div class="invisible err-name">Campo obrigatório!</div>
                                <input placeholder="DIGITE NOVO NOME DE USUÁRIO" type="text" name="name" id="name">
                                <button id="alterar-nome" class="btn-form">Alterar</button>
                            </form>
                            <form method="post" id="form-password">
    
                                <div class="invisible err-password">Campo obrigatório!</div>
                                <input placeholder="DIGITE NOVA SENHA" type="password" name="password" id="password">
    
                                <div class="invisible err-password1">Campo obrigatório!</div>
                                <input placeholder="DIGITE NOVA SENHA NOVAMENTE" type="password" name="password1" id="password-1">
                                <button id="alterar-senha" class="btn-form">Alterar</button>
                            </form>
                            <div class="cf-bnt">
                                <button id="confirme-alterar" class="btn-form">Confirmar alterações</button>
                            </div>
                        </div>
    
                    </div>
                </section>
            </section>
    
            <!-- CARTAO DE CREDITO -->
            <section class="credit-card">
                <h1 id="cadastrar" class="h1-t">Cadastrar novo Cartão</h1>
                
                <div class="card">
                    <div class="face-card">
                        <img src="../img/pagamento/chip.svg" alt="Chip" id="chip" class="absolute"></img>
                        <img src="../img/pagamento/signal.svg" alt="Signal" id="signal" class="absolute"></img>
    
                        <div id="card-info" class="absolute">
                            <div id="card-number">XXXX XXXX XXXX XXXX</div>
                           
                            <div class="flex">
                                <div class="flex-expiration">
                                    <div class="label">VALIDADE</div>
                                    <div class="value">XX/XX</div>
                                </div>
                                <div class="flex-security">
                                    <div class="label">CVC</div>
                                    <div class="value">XXX</div>
                                </div>
                            </div> 
                            <div class="label-titular">Nome do Titular</div>
                            <div id="value">JOAO DA SILVA</div>
                        </div>
                    </div>
                </div>
    
                <div class="new-card">
                    <div class="titulos">
                        <h2>Dados do novo cartão</h2>
                    </div>
                    
                    <div class="form-container active">
                        <form class="formulario">
                            <div class="invisible err-number">Campo obrigatório!</div>
                            <input placeholder="Número do cartão" type="text" name="number">
                            <div class="invisible err-name1">Campo obrigatório!</div>
                            <input placeholder="Nome completo" type="text" name="name1">
                            <div class="invisible err-expiry">Campo obrigatório!</div>
                            <input placeholder="MM/AA" type="text" name="expiry">
                            <div class="invisible err-cvc">Campo obrigatório!</div>
                            <input placeholder="CVC" type="text" name="cvc">
                            <button type="submit">Cadastrar</button>
                        </form>
                    </div>
                </div>
            </section>
    
    
            <!-- CANCELAR CONTA -->
            <section class="cancelar">
                <div>
                    <h1 id="cancelar" class="h1-t">Deseja cancelar sua assinatura?</h1>
                </div>
                
                <section class="cancel">
                    <a href="#modal" class="btn">Clique aqui para cancelar</a>
                    <div id="modal" class="modal">
                        <div class="modal_content">
                            <p>Assinatura cancelada com sucesso!</p>
                            <a href="#" class="modal_link">Ok</a>
                        </div>
                    </div>
                </section>
            </section>
        </div>
    </main>
</body>
